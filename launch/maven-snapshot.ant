<project default="snapshot" basedir="./..">

	<echo message="basedir : ${basedir}" />

	<target name="snapshot">
		<exec executable="mvn">
			<arg value="clean" />
			<arg value="deploy" />
			<arg value="--activate-profiles" />
			<arg value="com.carrotgarden.maven" />
		</exec>
	</target>

</project>
