<project default="maven" basedir="./..">

	<echo message="basedir : ${basedir}" />

	<target name="maven">
		<exec executable="mvn">
			<arg value="clean" />
			<arg value="package" />
			<arg value="--activate-profiles" />
			<arg value="amazon-distribution,amazon-repository" />
		</exec>
	</target>

</project>
